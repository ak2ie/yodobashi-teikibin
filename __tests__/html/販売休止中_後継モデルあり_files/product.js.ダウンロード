function relatedProductNoukiKaitouCallback(e){"use strict";var s;var a;var t;var r;var l;var g;var o;if(null!==e.ismult&&e.ismult)return void(location.href=e.redirecturl);for(a=e.item.length,s=0;s<a;s++)t=e.item[s],r=$("span[class^=processed-related-sku-"+t.sku+"]"),r.length&&(l='<div class="deliInfo">',t.stockMessage&&(g="",t.stockMessageStyle&&(g=' class="'+t.stockMessageStyle+'"'),l+=void 0!==g?t.deliveryTimeMessage&&t.deliveryDateMessage?"<strong"+g+">"+t.stockMessage+"。</strong>":"<strong"+g+">"+t.stockMessage+"</strong>":t.deliveryTimeMessage&&t.deliveryDateMessage?"<strong>"+t.stockMessage+"。</strong>":"<strong>"+t.stockMessage+"</strong>"),t.deliveryTimeMessage?l+=t.deliveryTimeMessage.replace(/span/g,"strong"):t.deliveryDateMessage&&(t.deliveryDelayDateMessage&&t.deliveryDelayDateMessage.length>0&&(l+=t.deliveryDelayDateMessage.replace(/span/g,"strong")),l+=t.deliveryDateMessage.replace(/span/g,"strong")+"に",t.carrierMessage&&(l+=t.carrierMessage.replace(/span/g,"strong")),l+="お届け"),l+="</div>",o=r.parent(),r.remove(),o.html(l),void 0!==t.stockStatusCode&&$("input[name=cartInSKU][value="+t.sku+"]").parent().children("input[name=stockStatusCode]").attr("value",t.stockStatusCode))}